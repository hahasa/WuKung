<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.mountx.wukung"
    tools:ignore="GoogleAppIndexingWarning">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".main.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".lists.ListWithConditionsActivity"
            android:label="@string/lists_activity_title"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoListChoiceIndicatorSingle" />

        <activity
            android:name=".layout.LayoutActivity"
            android:label="@string/layout_activity_title"
            android:screenOrientation="portrait" />

        <activity
            android:name=".drawable.CountdownActivity"
            android:label="@string/countdown_activity_title"
            android:screenOrientation="portrait" />
    </application>

</manifest>